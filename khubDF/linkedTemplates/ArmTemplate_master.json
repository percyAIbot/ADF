{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "khubDF"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"GitHub_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GitHub'"
		},
		"OnPremSQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPremSQL'"
		},
		"AthletesGitRaw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/percyAIbot/tokyo-olympic-azure-data-engineering-project/refs/heads/main/data/Athletes.csv"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://khubadfstorage.dfs.core.windows.net/"
		},
		"CoachesGitRaw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/percyAIbot/tokyo-olympic-azure-data-engineering-project/refs/heads/main/data/Coaches.csv"
		},
		"EntriesGenderGitRaw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/percyAIbot/tokyo-olympic-azure-data-engineering-project/refs/heads/main/data/EntriesGender.csv"
		},
		"GitHub_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "odl_user_1510914@cloudlabssandbox.onmicrosoft.com"
		},
		"MedalsGitRaw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/percyAIbot/tokyo-olympic-azure-data-engineering-project/refs/heads/main/data/Medals.csv"
		},
		"OnPremSQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"OnPremSQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2017"
		},
		"OnPremSQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sasidhar"
		},
		"TeamsGitRaw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/percyAIbot/tokyo-olympic-azure-data-engineering-project/refs/heads/main/data/Teams.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "khubDF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"GitHub_credential": {
						"value": "[parameters('GitHub_credential')]"
					},
					"AthletesGitRaw_properties_typeProperties_url": {
						"value": "[parameters('AthletesGitRaw_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"CoachesGitRaw_properties_typeProperties_url": {
						"value": "[parameters('CoachesGitRaw_properties_typeProperties_url')]"
					},
					"EntriesGenderGitRaw_properties_typeProperties_url": {
						"value": "[parameters('EntriesGenderGitRaw_properties_typeProperties_url')]"
					},
					"GitHub_properties_typeProperties_username": {
						"value": "[parameters('GitHub_properties_typeProperties_username')]"
					},
					"MedalsGitRaw_properties_typeProperties_url": {
						"value": "[parameters('MedalsGitRaw_properties_typeProperties_url')]"
					},
					"TeamsGitRaw_properties_typeProperties_url": {
						"value": "[parameters('TeamsGitRaw_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "khubDF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"OnPremSQL_password": {
						"value": "[parameters('OnPremSQL_password')]"
					},
					"OnPremSQL_properties_typeProperties_server": {
						"value": "[parameters('OnPremSQL_properties_typeProperties_server')]"
					},
					"OnPremSQL_properties_typeProperties_database": {
						"value": "[parameters('OnPremSQL_properties_typeProperties_database')]"
					},
					"OnPremSQL_properties_typeProperties_userName": {
						"value": "[parameters('OnPremSQL_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'khubDF_ArmTemplate_0')]"
			]
		}
	]
}